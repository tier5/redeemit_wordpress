				window.location.href = "<?php echo site_url(); ?>/index.php/deals/?deal_catid="+dcatid+"&deal_subcatid="+dsubcatid+"&deal_subsub_parent="+subsubparent+"&deal_loc="+location+"&cat_name="+cat_name+"&deal_tag="+dealtag;
			//}
			
			
      });

    });
</script>


