									 // 2. This code loads the IFrame Player API code asynchronously.
									  var tag = document.createElement('script');

									  tag.src = "https://www.youtube.com/iframe_api";
									  var firstScriptTag = document.getElementsByTagName('script')[0];
									  firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
									  function YouTubeGetID(url){
												  var ID = '';
												  if(url !== 'undefined'){
												  url = url.replace(/(>|<)/gi,'').split(/(vi\/|v=|\/v\/|youtu\.be\/|\/embed\/)/);
												  }
												  if(url[2] !== undefined) {
													ID = url[2].split(/[^0-9a-z_\-]/i);
													ID = ID[0];
												  }
												  else {
													ID = url;
												  }
													return ID;
									  }
									  // 3. This function creates an <iframe> (and YouTube player)
									  //    after the API code downloads.
										
										var vdo_ID = YouTubeGetID('<?php echo $latest_video_data_frst->post_content; ?>');
										
										var player;
									
									  function onYouTubeIframeAPIReady() 
									  {
										player = new YT.Player('player', {
										height: '313',
										width: '560',
										videoId: vdo_ID,
										events: {
										'onStateChange': onPlayerStateChange
										}
										});
									  }

										function onPlayerStateChange(e)
										{
											if(e.data == 0){
											$(".fa.vdo-paused").hide();
											$(".fa.vdo-starticon").show();
											}else if(e.data == 2){
											$(".fa.vdo-paused").hide();
											$(".fa.vdo-starticon").show();
											}else if(e.data == 1){
												//$(".vdo-start").attr("data-mode","");
												var video_data = player.getVideoData();
												var video_ID = video_data['video_id'];
												$('input.home_video_url').each(function() {
													var v_id = YouTubeGetID($(this).val());
													if((v_id === video_ID) && ($(this).siblings('.vdo-start').data("mode") == 1)){
														$(this).siblings('.vdo-start').children('.fa.vdo-starticon').hide();
														$(this).siblings('.vdo-start').children('.fa.vdo-paused').show();
														
													}
												});
												
											}
										 }
									
